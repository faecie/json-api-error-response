{
    "name": "faecie/json-api-error-response",
    "type": "symfony-bundle",
    "description": "The Bundle provides en error response as it described in JsonApi specification",
    "keywords": [
      "jsonapi",
      "rest",
      "fosrest",
      "jmsserializer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Umyarov Ruslan",
            "email": "umyarovrr@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Faecie\\Bundle\\JsonApiErrorResponseBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faecie\\Bundle\\JsonApiErrorResponseBundle\\Test\\": "tests"
        }
    },
    "require": {
        "friendsofsymfony/rest-bundle": "^2.3",
        "symfony/config": "^4.1",
        "jms/serializer": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    }
}
